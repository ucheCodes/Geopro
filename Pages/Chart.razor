        coneSeries = new LineSeries
        {
            Title = "con res.",
            MarkerType = MarkerType.Circle,
            Color = OxyColor.Parse("#00FF00")
        };
        coneModel.Series.Add(coneSeries);

        /*Initializing sleeve model*/ 
        sleeveXAxis = new LinearAxis(){Position = AxisPosition.Top, StartPosition = 0, EndPosition = 1,  MajorGridlineStyle = LineStyle.Solid, MinorGridlineStyle = LineStyle.Dot, Title = "Depth (m)"};
        sleeveModel.Axes.Add(sleeveXAxis);
        sleeveYAxis = new LinearAxis(){Position = AxisPosition.Left, StartPosition = 1, EndPosition = 0,  MajorGridlineStyle = LineStyle.Solid, MinorGridlineStyle = LineStyle.Dot, Title = "Penetration (kPa)"};
        sleeveModel.Axes.Add(sleeveYAxis);
        sleeveSeries = new LineSeries
        {
            Title = "con res.",
            MarkerType = MarkerType.Circle,
            Color = OxyColor.Parse("#00FF00")
        };
        sleeveModel.Series.Add(sleeveSeries);

        /*Initializing pore pressure model*/ 
       poreXAxis = new LinearAxis(){Position = AxisPosition.Top, StartPosition = 0, EndPosition = 1,  MajorGridlineStyle = LineStyle.Solid, MinorGridlineStyle = LineStyle.Dot, Title = "Depth (m)"};
       poreModel.Axes.Add(poreXAxis);
       poreYAxis = new LinearAxis(){Position = AxisPosition.Left, StartPosition = 1, EndPosition = 0,  MajorGridlineStyle = LineStyle.Solid, MinorGridlineStyle = LineStyle.Dot, Title = "Penetration (kPa)"};
       poreModel.Axes.Add(poreYAxis);
       poreSeries = new LineSeries
        {
            Title = "con res.",
            MarkerType = MarkerType.Circle,
            Color = OxyColor.Parse("#00FF00")
        };
        poreModel.Series.Add(poreSeries);
    }
